    <!-- 3. CAPA DE INFORMACIÓN (Pins + Radar) -->
    <g id="locations-layer" class="pointer-events-none"> <!-- Pins capturarán eventos ellos mismos -->
        {locations.map((loc) => (
            <g 
                class="group/pin cursor-pointer pointer-events-auto focus:outline-none"
                transform={`translate(${loc.x}, ${loc.y})`}
                tabindex="0"
                aria-label={`Ver información de ${loc.name}`}
            >
                <!-- Efecto de Radar (Animado en CSS) -->
                <circle r="40" fill="#3b82f6" class="radar-ping opacity-0" />
                <circle r="70" fill="#3b82f6" class="radar-ping opacity-0 delay-300" />
                
                <!-- Pin Body (Design System Jewel Style) -->
                <circle r="18" fill="white" class="drop-shadow-md dark:fill-slate-900" />
                <circle r="10" fill="#2563eb" class="transition-all duration-300 group-hover/pin:fill-blue-400 group-hover/pin:r-12" />
                
                <!-- Etiqueta Numérica Flotante -->
                <foreignObject x="-100" y="-80" width="200" height="60" class="overflow-visible">
                    <div class="flex justify-center opacity-0 translate-y-2 transition-all duration-300 group-hover/pin:opacity-100 group-hover/pin:translate-y-0 group-focus/pin:opacity-100 group-focus/pin:translate-y-0">
                        <div class="bg-slate-900/90 text-white text-xs px-3 py-1.5 rounded-full shadow-lg backdrop-blur-sm whitespace-nowrap border border-white/10">
                            <span class="font-bold">{loc.city}</span>
                        </div>
                    </div>
                </foreignObject>
            </g>
        ))}
    </g>
  </svg>

  <!-- TOOLTIP CONTAINER (Flotando fuera del SVG para usar HTML nativo rico) -->
  <div id="tooltip-container" class="absolute pointer-events-none transition-opacity duration-300 opacity-0 z-20 w-64">
      <!-- Template Glassmorphism -->
      <div class="bg-white/95 dark:bg-slate-900/95 backdrop-blur-xl p-4 rounded-2xl shadow-2xl border border-slate-200 dark:border-white/10 text-sm transform transition-transform duration-300 scale-95" id="tooltip-content">
          <h4 class="font-bold text-slate-900 dark:text-white text-lg mb-1" id="tt-name">Oficina</h4>
          <div class="h-px w-full bg-slate-100 dark:bg-slate-700 my-2"></div>
          <p class="text-slate-500 dark:text-slate-400 flex items-center gap-2">
              <span class="text-blue-500">📍</span> <span id="tt-city">Ciudad</span>
          </p>
          <a href="" id="tt-phone" class="mt-3 flex w-full items-center justify-center gap-2 rounded-full bg-blue-50 py-2 text-blue-600 font-bold text-xs hover:bg-blue-100 transition-colors pointer-events-auto">
              📞 <span id="tt-phone-text">Llamar</span>
          </a>
      </div>
  </div>

</div>

<!-- Datos pasados al cliente de forma segura -->
<script define:vars={{ locations }}>
  // Lógica de Tooltips con Performance (RequestAnimationFrame)
  document.addEventListener('astro:page-load', () => {
      const pins = document.querySelectorAll('.group\\/pin'); // Escape selector for /
      const tooltip = document.getElementById('tooltip-container');
      const content = document.getElementById('tooltip-content');
      const wrapper = document.getElementById('map-wrapper');
      
      // Referencias DOM internas del tooltip
      const ttName = document.getElementById('tt-name');
      const ttCity = document.getElementById('tt-city');
      const ttPhone = document.getElementById('tt-phone');
      const ttPhoneText = document.getElementById('tt-phone-text');

      if(!pins || !tooltip || !wrapper) return;

      pins.forEach((pin, index) => {
          const locData = locations[index];

          // Mouse Enter
          pin.addEventListener('mouseenter', (e) => {
              // 1. Popular Datos
              ttName.innerText = locData.name;
              ttCity.innerText = locData.city;
              ttPhoneText.innerText = locData.phone;
              ttPhone.href = `tel:${locData.phone.replace(/\s/g, '')}`;

              // 2. Calcular Posición (Relativa al SVG container)
              // Truco: Obtenemos las coordenadas del "Translate" del SVG group directamente
              // O mejor, usamos getBoundingClientRect para ser precisos en responsivo.
              const rect = pin.getBoundingClientRect();
              const wrapperRect = wrapper.getBoundingClientRect();

              // Calcular posición % relativa dentro del contenedor para que sea fluido
              const relX = rect.left - wrapperRect.left + (rect.width/2); 
              const relY = rect.top - wrapperRect.top;

              // Mover Tooltip
              tooltip.style.left = `${relX}px`;
              tooltip.style.top = `${relY - 10}px`; // Un poco arriba del pin
              tooltip.style.transform = 'translate(-50%, -100%)'; // Centrado horizontal, arriba vertical

              // 3. Mostrar
              tooltip.classList.remove('opacity-0');
              content.classList.remove('scale-95');
              content.classList.add('scale-100');
          });

          // Mouse Leave
          pin.addEventListener('mouseleave', () => {
              tooltip.classList.add('opacity-0');
              content.classList.remove('scale-100');
              content.classList.add('scale-95');
          });
      });
  });
  
  // Fallback inicial
  document.dispatchEvent(new Event('astro:page-load')); 
</script>

<style>
  /* ANIMACIONES CUSTOM OPTIMIZADAS */
  
  .radar-ping {
      transform-origin: center;
      animation: radarWave 3s cubic-bezier(0, 0, 0.2, 1) infinite;
  }
  
  .delay-300 {
      animation-delay: 1s;
  }

  @keyframes radarWave {
      0% {
          transform: scale(0.1);
          opacity: 0;
      }
      50% {
          opacity: 0.4;
      }
      100% {
          transform: scale(2);
          opacity: 0;
      }
  }
</style>